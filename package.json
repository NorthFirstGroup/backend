{
    "name": "go-ticket-backend",
    "version": "2.0.12",
    "main": "server.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "dev": "nodemon",
        "build": "tsc && tsc-alias",
        "start": "node dist/server.js",
        "startdocker": "docker compose --env-file .env up -d --build",
        "restart": "docker compose --env-file .env up --force-recreate --build -d",
        "stop": "docker compose --env-file .env stop",
        "clean": "docker compose --env-file .env down -v",
        "seeds": "ts-node -r tsconfig-paths/register src/seeds/seeds.ts",
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint . --ext .ts --fix",
        "format": "prettier --write \"src/**/*.ts\"",
        "version:major": "export ENV_ASSIGN='major' && chmod +x ./version-up.sh && ./version-up.sh",
        "version:patch": "export ENV_ASSIGN='patch' && chmod +x ./version-up.sh && ./version-up.sh"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "description": "",
    "devDependencies": {
        "@eslint/js": "^9.24.0",
        "@types/bcrypt": "^5.0.2",
        "@types/cors": "^2.8.17",
        "@types/express": "^5.0.1",
        "@types/formidable": "^3.4.5",
        "@types/ioredis": "^4.28.10",
        "@types/jsonwebtoken": "^9.0.9",
        "@types/node": "^22.14.1",
        "@types/nodemailer": "^6.4.17",
        "@types/redis": "^4.0.10",
        "@typescript-eslint/eslint-plugin": "^8.29.1",
        "@typescript-eslint/parser": "^8.29.1",
        "eslint": "^9.24.0",
        "globals": "^16.0.0",
        "nodemon": "^3.1.9",
        "prettier": "^3.5.3",
        "ts-node": "^10.9.2",
        "tsc-alias": "^1.8.16",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.8.3",
        "typescript-eslint": "^8.29.1"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.808.0",
        "@aws-sdk/s3-request-presigner": "^3.808.0",
        "bcrypt": "^5.1.1",
        "cors": "^2.8.5",
        "dayjs": "^1.11.13",
        "dotenv": "^16.5.0",
        "express": "^5.1.0",
        "express-jwt": "^8.5.1",
        "formidable": "^3.5.4",
        "ioredis": "^5.6.0",
        "jsonwebtoken": "^9.0.2",
        "node-ecpay-aio": "^0.2.3",
        "nodemailer": "^7.0.0",
        "pg": "^8.14.1",
        "pino": "^9.6.0",
        "pino-http": "^10.4.0",
        "pino-pretty": "^13.0.0",
        "redis": "^4.7.0",
        "reflect-metadata": "^0.2.2",
        "typeorm": "^0.3.22"
    }
}
